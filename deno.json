{
  "tasks": {
    "serve": "deno run --watch --allow-read --allow-net build/index.js"
  },
  "deploy": {
    "project": "chy-data-explorer",
    "exclude": [
      "**/node_modules",
      "**/static"
    ],
    "include": [
      "./serve.ts",
      "./build/*"
    ],
    "entrypoint": "build/index.js"
  }
}